<h1>New contract for <%= @character.name %></h1>
 
<% form_for ([@character,@character_contract], :url => character_contracts_path(@character), :html => {:method => :post}) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit "Add Character Contract" %>
<% end %>
<%= link_to '[Back to List of Characters]', characters_path %>
